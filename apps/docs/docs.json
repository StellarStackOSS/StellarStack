{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "almond",
  "name": "StellarStack",
  "colors": {
    "primary": "#FAFAFA",
    "light": "#FAFAFA",
    "dark": "#0A0A0A"
  },
  "favicon": "/favicon.svg",
  "logo": {
    "dark": "/logo/dark.png",
    "light": "/logo/light.png"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "introduction",
              "guides/installation",
              "guides/quickstart",
              "guides/development"
            ]
          },
          {
            "group": "Core Guides",
            "pages": [
              "guides/servers",
              "guides/files",
              "guides/backups",
              "guides/databases",
              "guides/network",
              "guides/schedules"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Getting Started",
            "pages": ["api-reference/introduction", "api-reference/authentication"]
          },
          {
            "group": "Endpoints",
            "pages": ["api-reference/servers", "api-reference/files", "api-reference/backups"]
          }
        ]
      },
      {
        "tab": "Changelog",
        "groups": [
          {
            "group": "Releases",
            "pages": ["changelog/releases"]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "GitLab",
          "href": "https://gitlab.com/StellarStackOSS/stellarstack",
          "icon": "gitlab"
        },
        {
          "anchor": "Discord",
          "href": "https://discord.stellarstack.app",
          "icon": "discord"
        }
      ]
    }
  },
  "navbar": {
    "links": [
      {
        "label": "GitLab",
        "href": "https://gitlab.com/StellarStackOSS/stellarstack"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get Started",
      "href": "https://gitlab.com/StellarStackOSS/stellarstack"
    }
  },
  "footer": {
    "socials": {
      "gitlab": "https://gitlab.com/StellarStackOSS/stellarstack",
      "discord": "https://discord.stellarstack.app"
    }
  },
  "redirects": [
    {
      "source": "/",
      "destination": "/introduction"
    }
  ]
}
